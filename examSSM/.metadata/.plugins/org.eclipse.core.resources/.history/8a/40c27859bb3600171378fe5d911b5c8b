package java8Test;

public class LambdaScopeTest {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 String  s = "Hello";
		greetHello g=mesg->System.out.println(s+mesg);
		//s="ll";
		g.sayHello(" Lisa");
	}
	interface greetHello{
		void sayHello(String message);
	}

}
